if s > 86400:
    print("输入不正确")
elif M >=0 and S > 0:
    print("时间为："'%2d:%.2d:%.2d'% (23 - H,59 - M,60 - S))
elif S==0 and M==0:          
    print("时间为："'%2d:%.2d:%.2d'%(24 - H,M, S))
elif M >0 and S >= 0:
    print("时间为："'%2d:%.2d:%.2d'%(23-H,60-M,S))
