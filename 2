'''
x=float(input("请输入坐标x:"))
y=float(input("请输入坐标y:"))
if ((x-2)**2+(y-2)**2)**0.5<1:
    print('在第一个圆内')
elif ((x+2)**2+(y-2)**2)**0.5<1:
    print('在第二个圆内')
elif ((x+2)**2+(y+2)**2)**0.5<1:
    print('在第三个圆内')
elif ((x-2)**2+(y+2)**2)**0.5<1:
    print('在第四个圆内')
else:
    print("不在任何一个圆内")

